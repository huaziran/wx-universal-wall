<block wx:for="{{dataList}}" wx:key="index">
  <view data-itme="{{itme}}">
    <image src="{{item.name.avatarUrl}}" class="imgtop"></image>
    <view>名称：{{item.name.nickName}}</view>
    <view>动态：{{item.text}}</view>
    <view>动态：{{item.time}}</view>
    <image src="{{dianzanUrl}}" bindtap="dianzantop" class="img"></image>
    <text>{{index}}</text>
    <image src="{{shoucangUrl}}" bindtap="shoucangtop" class="img"></image>
    <text>{{scindex}}</text>
    <view>-------------</view>
  </view>
</block>